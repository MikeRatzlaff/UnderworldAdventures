digraph G {
{
   node [shape=box,fontsize=10,height=0.2,width=0.4,fontname="Helvetica",style=filled,color=".7 .3 1.0"];

   "Base" [URL="\ref Base"];
   "Base" -> "ZzipLib"

   "Audio" [URL="\ref Audio"];
   "Audio" -> "Base";
  
   "Conv" [URL="\ref Conv"];
   "Conv" -> "Base";

   "Import" [URL="\ref Import"];
   "Import" -> "Base";
   "Import" -> "Underw";

   "Lua" [URL="\ref Lua"];

   "Renderer" [URL="\ref Renderer"];
   "Renderer" -> "Base";
   
   "Screens" [URL="\ref Screens"];
   "Screens" -> "Base";
   "Screens" -> "UI";
   "Screens" -> "Script";

   "Script" [URL="\ref Script"];
   "Script" -> "Base";
   "Script" -> "Lua";
   "Script" -> "Underw";

   "UI" [URL="\ref UserInterface"];
   "UI" -> "Base"
   "UI" -> "Renderer"

   "Underw" [URL="\ref Underworld"];
   "Underw" -> "Base";
   
   "ZzipLib";
}
